    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-import": "^1.13.5",
    "chalk": "4.1.2",
    "commitlint": "^17.0.0",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
