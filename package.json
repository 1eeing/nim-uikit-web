  "license": "MIT",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean",
    "dev": "lerna run dev",
    "build": "lerna run build",
    "start": "yarn storybook",
    "start:prod": "STORYBOOK_ENV=prod yarn storybook",
    "p": "yarn test && yarn build && lerna publish --conventional-commits --yes --no-private",
    "test": "jest --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "lerna run build && build-storybook",
    "build-storybook-prod": "lerna run build && STORYBOOK_ENV=prod build-storybook",
    "prepare": "husky install",
    "eslint": "eslint packages --fix",
    "cz": "cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "noIssue": "node ./build/changelogNoIssue.js",
    "opensource": "node ./build/openSource.js",
    "pack:im": "lerna run build && node ./build/copyIm.js"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "devDependencies": {
